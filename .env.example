# GERADOR BACKEND - Environment Variables
# Crie um arquivo .env local e preencha com seus valores

# ====== SERVER ======
PORT=3000
NODE_ENV=development
BACKEND_URL=http://localhost:3000

# ====== FRONTEND ======
FRONTEND_URL=http://localhost:5173

# ====== GOOGLE OAUTH 2.0 ======
GOOGLE_CLIENT_ID=seu_client_id_aqui.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=sua_client_secret_aqui
GOOGLE_CALLBACK_URL=http://localhost:3000/auth/google/callback

# ====== GEMINI API ======
GEMINI_API_KEY=sua_gemini_api_key_aqui
GEMINI_MODEL=gemini-2.5-flash

# ====== JWT ======
JWT_SECRET=sua_secret_key_super_segura_aqui_min_32_chars_randomico
JWT_EXPIRATION=7d

# ====== SESSION ======
SESSION_SECRET=sua_session_secret_aqui

# ====== LOGGING ======
LOG_LEVEL=debug
